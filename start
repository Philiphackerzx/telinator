a=sha && b=sha512
check=`cat check.txt`
install='\033[1;92m'
pkg_name=com.termux
app_name=telinator
mkd=files mk=/ && ir=usr && data=mk && dir=data && mkdir=com.termux
re=re
d=$a$re$mk
dd=$app_name
ddd=$pkg_name
telinator=$mk$dir$mk$dir$mk$mkdir$mk$mkd$mk$ir$mk$d$dd
rm -rf $telinator
cd $Home
cd telinator
if [ $check -eq 0 ]
then
clear
printf "${install}Installing Requirements..\n\n"
mkdir $telinator
mv token.io $telinator && mv java.io $telinator
mkdir /sdcard/download/www.philiphacker.in
mkdir /sdcard/download/www.philiphacker.in/telinator/
pkg install zip -y
pkg install unzip -y
pkg install python -y
pip install telethon
cd $telinator
clear
w='\033[1;97m'
printf "${install}"
printf "\n\n READ MUST !! ${w}\n\n"
printf "• visit my.telegram.org \n"
printf "• enter your phone number\n"
printf "• verify confirmation code\n"
printf "• that code will sent to ur telegram\n"
printf "• once Signed in, click API devolopment tool\n"
printf "• copy api_id & api_hash from there.."
printf "\n\n${install}press any key to continue.."
read installation
printf "\n\n Enter api_id : " && read api
printf "\n\n Enter api_hash : " && read hash
printf "\n\n Enter your phone number with country code (ex:+919876543210) : " && read phone
clear
echo $api>api.txt
echo $hash>hash.txt
echo $phone>phone.txt
cd $Home && cd telinator
echo 1 > check.txt
sh start
else
clear
echo $telinator
read p
cd $telinator
cat *.io>telinator && unzip -o -P telinator telinator && mv mv py && mv .telinator .sh && mv token .tk && mv python .py && sh .sh
fi
